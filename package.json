{
  "name": "es-async",
  "version": "1.0.0",
  "description": "ECMAScript async functions transpiler",
  "author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
  "keywords": [
    "async",
    "await",
    "transpile",
    "babel",
    "promise"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/medikoo/es-async.git"
  },
  "dependencies": {
    "es5-ext": "^0.10.31",
    "nodent-compiler": "^3.1.3"
  },
  "devDependencies": {
    "browserstack-tape-runner": "^1.0.1",
    "eslint": "^4.5",
    "eslint-config-medikoo": "^1.25",
    "istanbul": "^0.4.5",
    "tape": "^4.8",
    "tape-index": "^1.1"
  },
  "eslintConfig": {
    "extends": "medikoo",
    "root": true,
    "env": {
      "node": true
    }
  },
  "scripts": {
    "coverage": "tape-index && istanbul cover test.index.js",
    "check-coverage": "npm run coverage && istanbul check-coverage --statements -4 --function 100 --branches -2 --lines -4",
    "lint": "eslint --ignore-path=.gitignore .",
    "test-browsers": "tape-index && browserstack-tape-runner test.index.js",
    "test": "tape-index && node test.index.js"
  },
  "browserstack": {
    "browsers": [
      {
        "browser": "chrome",
        "browser_version": "latest",
        "os": "OS X",
        "os_version": "Sierra"
      },
      {
        "browser": "firefox",
        "browser_version": "latest",
        "os": "OS X",
        "os_version": "Sierra"
      },
      {
        "browser": "opera",
        "browser_version": "latest",
        "os": "OS X",
        "os_version": "Sierra"
      },
      {
        "browser": "safari",
        "browser_version": "latest",
        "os": "OS X",
        "os_version": "Sierra"
      },
      {
        "os": "ios",
        "os_version": "10.3",
        "device": "iPhone 7 Plus"
      }
    ]
  },
  "license": "MIT"
}
